<!DOCTYPE html>
<html>
<head>
  <title>TravelDiary</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <h1 class="header"><%= link_to 'Travel Diary', root_path, {:style=>"text-decoration: none;"} %></h1>
    <div style="text-align: center;">
        <span><%= link_to 'Cities', cities_path, {:style=>"text-decoration: none;"} %></span> |
        <span><%= link_to ' Reviews', reviews_path, {:style=>"text-decoration: none;"} %></span>
    </div>
    <div id="cotent">
        <%= yield %>
    </div>
</body>
</html>
